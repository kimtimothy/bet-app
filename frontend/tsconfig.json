{
  "compilerOptions": {
    "target": "es2016",
    "module": "esnext",
    "jsx": "react-native",
    "lib": ["dom", "es2016"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": ["src", "App.tsx"],
  "exclude": ["node_modules"]
}